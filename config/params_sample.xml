<batches>
    <batch name="Trains">
        <team type="Train" num="17"/>
        <shift start="01-01 9:00" stop="01-01 17:00"></shift>
        <traffic>m,m,m,m,m,m,m,m,m</traffic>
    </batch>
</batches>

<teams>
    <team name="Train">
        <!-- Agents -->
        <agents>
            <agent name="engineer" task_ids="0,1"/>
            <agent name="conductor" task_ids="1"/>
            <agent name="positive train control" task_ids="1"/>
            <agent name="cruise control" task_ids="0"/>
        </agents>
        <!-- Tasks -->
        <tasks>
            <task name="Task1" id="0">
                <priority>4</priority>
                <isAffectedByTraffic>0</isAffectedByTraffic>
                <interarrival type="exponential" lambda="0.01"/>
                <service type="lognormal" mean="10" stddev="0"/>
                <expiration type="uniform" min="0" max="2"/>
            </task>
            <task name="Task2" id="1">
                <priority>4</priority>
                <isAffectedByTraffic>0</isAffectedByTraffic>
                <interarrival type="exponential" lambda="0.01"/>
                <service type="lognormal" mean="10" stddev="0"/>
                <expiration type="uniform" min="0" max="2"/>
            </task>
        </tasks>
    </team>
</teams>
