<?xml version="1.0" encoding="UTF-8"?>
<parameters>
    <output_path>/Users/Branch/Documents/Research/fra/show/out</output_path>
    <replications>100</replications>
</parameters>

<teams>
    <team name="Train">
        <name>
        <phases num="3">30, 90, -30</phases>
        <!-- Agents -->
        <agents>
            <agent name="engineer" task_ids="0,1" default="true"></agent>
            <agent name="conductor" task_ids="1"></agent>
            <agent name="positive train control" task_ids="1"></agent>
            <agent name="cruise control" task_ids="0"></agent>
        </agents>
        <!-- Tasks -->
        <tasks>
            <task id="0" type="independent">
                <!-- max number of task types -->
                <name>My Task</name>
                <priority>4, 3, 4</priority>
                <isAffectedByTraffic>0, 1, 0</isAffectedByTraffic>
                <interarrival type="exponential" byPhase="true">
                    <phase>10</phase>
                    <phase>10</phase>
                    <phase>10</phase>
                </interarrival>
                <service type="lognormal">0,2</service>
                <expiration type="uniform" byPhase="true">
                    <phase>0,2</phase>
                    <phase>0,2</phase>
                    <phase>0,2</phase>
                </expiration>
                <subtasks>
                    <!-- subtask adds new tasktype -->
                    <subtask team="Train" id="1"></subtask>
                    <subtask team="Dispatch" id="0"></subtask>
                    <subtask team="Train" id="1"></subtask>
                </subtasks>
            </task>
            <task id="1" type="dependent">
                <name>My Task</name>
                <priority>4, 3, 4</priority>
                <isAffectedByTraffic>0, 1, 0</isAffectedByTraffic>
                <interarrival type="exponential" byPhase="true">
                    <phase>0</phase>
                    <phase>0</phase>
                    <phase>0</phase>
                </interarrival>
                <service type="lognormal">0,2</service>
                <expiration type="uniform" byPhase="true">
                    <phase>0,2</phase>
                    <phase>0,2</phase>
                    <phase>0,2</phase>
                </expiration>
            </task>
        </tasks>
    </team>
    <team name="Dispatch">
        <!-- Agents -->
        <agents>
            <agent name="dispatcher" task_ids="0,1" default="true"></agent>
        </agents>
        <!-- Tasks -->
        <tasks>
            <task id="0">
                <name>My Task</name>
                <priority>4, 3, 4</priority>
                <isAffectedByTraffic>0, 1, 0</isAffectedByTraffic>
                <interarrival type="exponential" byPhase="true">
                    <phase>10</phase>
                    <phase>0.1</phase>
                    <phase>0.1</phase>
                </interarrival>
                <service type="lognormal">0,2</service>
                <expiration type="uniform" byPhase="true">
                    <phase>0,2</phase>
                    <phase>0,2</phase>
                    <phase>0,2</phase>
                </expiration>
                <interaction>
                    <subtask team="Dispatch" id=""></subtask>
                </interaction>
            </task>
            <task id="1">
                <name>My Task</name>
                <priority>4, 3, 4</priority>
                <isAffectedByTraffic>0, 1, 0</isAffectedByTraffic>
                <interarrival type="exponential" byPhase="true">
                    <phase>0</phase>
                    <phase>0</phase>
                    <phase>0</phase>
                </interarrival>
                <service type="lognormal">0,2</service>
                <expiration type="uniform" byPhase="true">
                    <phase>0,2</phase>
                    <phase>0,2</phase>
                    <phase>0,2</phase>
                </expiration>
                <dependency>
                </dependency>
            </task>
        </tasks>
    </team>
</teams>
